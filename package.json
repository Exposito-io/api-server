{
  "name": "api-server",
  "version": "0.0.1",
  "description": "An express REST API seed written in TypeScript.",
  "main": "bin/www.ts",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "./node_modules/.bin/tsc",
    "prestart": "npm run build",
    "start": "node ./build/bin/www.js",
    "predev": "npm run clean",
    "dev": "concurrently \"npm run tsc-watch\" \"npm run nodemon\"",
    "prenodemon": "wait-on ./build/bin/www.js",
    "nodemon": "nodemon --inspect --delay 1500ms --watch ./build \"./build/bin/www.js\"",
    "tsc-watch": "./node_modules/.bin/tsc -w",
    "lint": "tslint -c tslint.json \"./src/**/*.ts\"",
    "test": "./node_modules/.bin/mocha -r ts-node/register test/**/*.spec.ts"
  },
  "author": "Mathew Cormier <mathew.corm@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/giannico/typescript-express-rest-seed.git"
  },
  "keywords": [
    "express",
    "typescript",
    "rest",
    "api",
    "types"
  ],
  "engines": {
    "node": "8.x",
    "npm": "4.x"
  },
  "dependencies": {
    "@google-cloud/compute": "^0.7.2",
    "app-root-path": "^1.3.0",
    "bignumber.js": "^4.0.2",
    "bitcore-lib": "^0.14.0",
    "bitcore-wallet-client": "^5.1.2",
    "body-parser": "1.15.2",
    "bull": "^3.1.0",
    "cache-manager": "^2.4.0",
    "cache-manager-redis": "^0.4.0",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^3.3.0",
    "cookie-parser": "1.4.3",
    "core-js": "2.4.1",
    "cors": "^2.8.4",
    "ejs": "2.5.7",
    "ethereumjs-tx": "^1.3.3",
    "express": "4.14.0",
    "express-bearer-token": "^2.1.0",
    "express-session": "^1.15.2",
    "github": "^10.1.0",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mongo-factory": "^1.0.0",
    "mongodb": "^2.2.30",
    "morgan": "1.7.0",
    "node-ts-cache": "0.0.7",
    "npmlog": "^4.1.0",
    "open-exchange-rates": "^0.3.0",
    "passport": "^0.3.2",
    "passport-google-oauth2": "^0.1.6",
    "preconditions": "^2.2.0",
    "read": "^1.0.7",
    "reflect-metadata": "^0.1.10",
    "serialize-error": "2.0.0",
    "sjcl": "^1.0.6",
    "socket.io": "^2.0.3",
    "uuid": "^3.0.1",
    "validate.js": "0.10.0",
    "web3": "^0.20.3",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bignumber.js": "^4.0.2",
    "@types/body-parser": "0.0.33",
    "@types/bull": "^3.0.0",
    "@types/cache-manager": "^1.2.4",
    "@types/chai": "^3.5.2",
    "@types/connect-redis": "0.0.5",
    "@types/cookie-parser": "^1.3.30",
    "@types/core-js": "0.9.34",
    "@types/debug": "0.0.29",
    "@types/express": "4.0.33",
    "@types/github": "^7.1.0",
    "@types/jsonwebtoken": "^7.2.1",
    "@types/lodash": "^4.14.64",
    "@types/mocha": "^2.2.41",
    "@types/mongodb": "^2.2.2",
    "@types/morgan": "1.7.32",
    "@types/node": "^7.0.22",
    "@types/passport": "^0.3.3",
    "@types/read": "0.0.28",
    "@types/reflect-metadata": "0.0.5",
    "@types/sjcl": "^1.0.28",
    "@types/socket.io": "^1.4.29",
    "@types/uuid": "^2.0.29",
    "@types/validator": "5.7.34",
    "@types/winston": "0.0.28",
    "chai": "^3.5.0",
    "concurrently": "3.1.0",
    "dotenv": "2.0.0",
    "mocha": "^3.4.2",
    "nodemon": "1.11.0",
    "rimraf": "2.5.4",
    "ts-node": "^3.0.4",
    "tslint": "3.15.1",
    "typescript": "^2.5.3",
    "wait-on": "1.5.3"
  }
}
